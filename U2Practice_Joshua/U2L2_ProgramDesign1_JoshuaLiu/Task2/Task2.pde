//Draws a series of line with increasing space between them
void setup(){
  size(800,500);
  design();
}
void design(){
  int a=1;
  line(100,a,700,a);
  a=a*3;
  line(100,a,700,a);
  a=a*3;
  line(100,a,700,a);
  a=a*3;
  line(100,a,700,a);
  a=a*3;
  line(100,a,700,a);
  a=a*3;
  line(100,a,700,a);
}
